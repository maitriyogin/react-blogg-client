{
  "name": "react-blogg-client",
  "version": "1.0.0",
  "description": "React blogg client, for the react blogg server.  Primary purpose is to demonstrate redux both on the server and in the client",
  "main": "index.js",
  "scripts": {
    "start": "node server",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.@(js|jsx)'",
    "test:watch": "npm run test -- --watch",
    "build": "NODE_ENV=production UV_THREADPOOL_SIZE=100 webpack -p --config webpack.production.config.js",
    "postinstall": "node deploy"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^3.3.0",
    "chai-immutable": "^1.3.0",
    "jsdom": "^6.5.1",
    "mocha": "^2.3.3",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.19.0",
    "deep-equal": "^1.0.1",
    "es6-promise": "^3.0.2",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "fbjs": "^0.2.1",
    "history": "^1.12.5",
    "http": "0.0.0",
    "http-proxy": "^1.11.2",
    "immutable": "^3.7.5",
    "isomorphic-fetch": "^2.1.1",
    "mkdirp": "^0.5.0",
    "moment": "^2.10.6",
    "node-sass": "^3.3.3",
    "react": "^0.13.3",
    "react-dom": "^0.14.0-rc1",
    "react-redux": "^3.0.1",
    "react-router": "^1.0.0-rc1",
    "redux": "^3.0.2",
    "redux-devtools": "^2.1.5",
    "redux-react-router": "^0.4.0",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^1.0.0",
    "rmdir": "^1.1.0",
    "sass-loader": "^3.0.0",
    "showdown": "^1.2.3",
    "socket.io-client": "^1.3.7",
    "style-loader": "^0.12.4"
